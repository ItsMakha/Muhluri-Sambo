{
  "version": 2,
  "builds": [
    {
      "src": "muhluri/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "100mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/send-email.js"
    },
    {
      "src": "/(.*)",
      "dest": "muhluri/wsgi.py"
    }
  ]
}