{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/send-email.js"
    },
    {
      "src": "/(.*)",
      "dest": "muhluri_portfolio/$1"
    }
  ]
}